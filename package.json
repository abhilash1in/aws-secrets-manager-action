{
  "name": "aws-secrets-manager-action",
  "version": "v1",
  "description": "Use secrets from AWS Secrets Manager as environment variables in your GitHub Actions workflow with name prefix support",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "package": "ncc build src/index.ts --license licenses.txt --source-map -o dist",
    "test": "npm run lint && jest --verbose --coverage=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suriyanto/aws-secrets-manager-action.git"
  },
  "keywords": [
    "Github",
    "Actions",
    "JavaScript",
    "AWS",
    "AWS Secrets Manager"
  ],
  "author": "Abhilash Kishore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suriyanto/aws-secrets-manager-action/issues"
  },
  "homepage": "https://github.com/suriyanto/aws-secrets-manager-action#readme",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "aws-sdk": "^2.1111.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vercel/ncc": "^0.33.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-plugin-spellcheck": "0.0.19",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "typescript": "4.6.2"
  }
}
